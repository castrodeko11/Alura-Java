package bytebank;

public class Conta {
	double saldo;
	int agencia;
	int numero;
	String titular;

	void deposita(double valor) {
		this.saldo += valor;

	}

	public boolean saca(double valor) {
		if (saldo > 0) {
			saldo -= valor;
			return true;
		} else {
			System.out.println("Saldo insuficiente");
			return false;
		}
	}

}
